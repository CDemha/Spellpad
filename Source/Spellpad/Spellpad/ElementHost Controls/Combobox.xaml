<UserControl x:Class="Spellpad.Combobox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Spellpad"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
        <ComboBox x:Name="comboBox" Height="22" VerticalAlignment="Top" IsEditable="True" IsTextSearchCaseSensitive="True" TextOptions.TextFormattingMode="Display"/>


</UserControl>
